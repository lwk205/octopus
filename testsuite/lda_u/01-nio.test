# -*- coding: utf-8 mode: shell-script -*-

Test       : GGA+U for (AF) bulk NiO crystal
Program    : octopus
TestGroups : long-run, periodic_systems, lda_u
Enabled    : Yes

Input      : 01-nio.01-U5-gs.inp

match ; SCF convergence ; GREPCOUNT(static/info, 'SCF converged') ; 1

match ; Total k-points   ; GREPFIELD(static/info, 'Total number of k-points', 6) ; 8
match ; Reduced k-points ; GREPFIELD(static/info, 'Number of symmetry-reduced k-points', 6 ) ; 4

Precision: 5e-05
match ; Total energy        ; GREPFIELD(static/info, 'Total       =', 3) ; -246.66825479
Precision: 0.0001
match ; Ion-ion energy     ; GREPFIELD(static/info, 'Ion-ion     =', 3) ; -177.00987193
match ; Eigenvalues sum    ; GREPFIELD(static/info, 'Eigenvalues =', 3) ; -32.71700903
Precision: 6.908549999877778e-05
match ; Hartree energy      ; GREPFIELD(static/info, 'Hartree     =', 3) ; 46.73654390
Precision: 0.0001
match ; Exchange energy    ; GREPFIELD(static/info, 'Exchange    =', 3) ; -27.72163558 
match ; Correlation energy ; GREPFIELD(static/info, 'Correlation =', 3) ; -2.03834620
Precision: 6.0439500002473786e-05
match ; Kinetic energy      ; GREPFIELD(static/info, 'Kinetic     =', 3) ; 123.21302584
Precision: 0.0001271104999929662
match ; External energy     ; GREPFIELD(static/info, 'External    =', 3) ; -210.06279082
Precision: 0.0001
match ; Hubbard energy     ; GREPFIELD(static/info, 'Hubbard     =', 3) ;  0.21687544
match ; Fermi energy       ; GREPFIELD(static/info, 'Fermi energy =', 4);  0.416062
match ; Total Magnetic Moment ; GREPFIELD(static/info, 'mz = ', 3);      0.0000 
match ; Local Magnetic Moment (Ni1) ; GREPFIELD(static/info, '1        Ni', 3);  3.314003
match ; Local Magnetic Moment (Ni2) ; GREPFIELD(static/info, '2        Ni', 3); -3.314004
match ; Local Magnetic Moment (O1) ;  GREPFIELD(static/info, '3         O', 3);  0.0000 
match ; Local Magnetic Moment (O2) ;  GREPFIELD(static/info, '4         O', 3);  0.0000 

match ; k-point 1 (x) ; GREPFIELD(static/info, '#k =   1', 7) ; 0.000000 
match ; k-point 1 (y) ; GREPFIELD(static/info, '#k =   1', 8) ; 0.000000
match ; k-point 1 (z) ; GREPFIELD(static/info, '#k =   1', 9) ; 0.000000
match ; Eigenvalue 1 up ; GREPFIELD(static/info, '#k =   1', 3, 1) ;  -3.025004
match ; Eigenvalue 4 dn ; GREPFIELD(static/info, '#k =   1', 3, 8) ;  -2.898172 
match ; Eigenvalue 8 dn ; GREPFIELD(static/info, '#k =   1', 3, 16) ; -0.438205
match ; Eigenvalue 9 up ; GREPFIELD(static/info, '#k =   1', 3, 17) ; -0.164686 

match ; Occupation Ni2 down 3d4 ; LINEFIELD(static/occ_matrices, -2, 7) ; 0.87302059
match ; Occupation Ni2 down 3d5 ; LINEFIELD(static/occ_matrices, -1, 9) ; 0.88497089

Input      : 01-nio.02-unocc.inp
match ; Red. coord. k1 ; LINEFIELD(static/bandstructure-sp1, 2, 1) ;  0.00000000
match ; Band structure k1x ; LINEFIELD(static/bandstructure-sp1, 2, 2) ;  0.00000000
match ; Band structure k1y ; LINEFIELD(static/bandstructure-sp1, 2, 3) ;  0.00000000
match ; Band structure k1z ; LINEFIELD(static/bandstructure-sp1, 2, 4) ;  0.00000000
match ; Band structure E1(k1) ; LINEFIELD(static/bandstructure-sp1, 2, 5) ;  -3.02500042
match ; Band structure E2(k1) ; LINEFIELD(static/bandstructure-sp1, 2, 6) ;  -3.02379715
match ; Band structure E3(k1) ; LINEFIELD(static/bandstructure-sp1, 2, 7) ;  -3.01700182

match ; Red. coord. k3 ; LINEFIELD(static/bandstructure-sp1, 4, 1) ;   0.2500000
match ; Band structure k3x ; LINEFIELD(static/bandstructure-sp1, 4, 2) ;  -0.01562000
match ; Band structure E1(k3) ; LINEFIELD(static/bandstructure-sp1, 4, 5) ;  -3.02500119
match ; Band structure E2(k3) ; LINEFIELD(static/bandstructure-sp1, 4, 6) ;  -3.02379736
match ; Band structure E3(k3) ; LINEFIELD(static/bandstructure-sp1, 4, 7) ;  -3.01700157

