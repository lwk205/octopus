# -*- coding: utf-8 mode: shell-script -*-

Test       : ACBN0 functional for (AF) NiO and LiF bulk crystals
Program    : octopus
TestGroups : long-run, periodic_systems, lda_u
Enabled    : Yes


Input      : 02-ACBN0.01-nio.inp

Precision : 0.0001

match ; SCF convergence ; GREPCOUNT(static/info, 'SCF converged') ; 1

match ; Total k-points   ; GREPFIELD(static/info, 'Total number of k-points', 6) ; 4

Precision : 0.0003
match ; Total energy       ; GREPFIELD(static/info, 'Total       =', 3) ;  -284.91272034
match ; Ion-ion energy     ; GREPFIELD(static/info, 'Ion-ion     =', 3) ;  -177.00987193 
Precision : 0.0006
match ; Eigenvalues sum    ; GREPFIELD(static/info, 'Eigenvalues =', 3) ;  -35.54624530
Precision : 0.00015
match ; Hartree energy     ; GREPFIELD(static/info, 'Hartree     =', 3) ;   87.46452925 
match ; Exchange energy    ; GREPFIELD(static/info, 'Exchange    =', 3) ;  -33.65236383 
match ; Correlation energy ; GREPFIELD(static/info, 'Correlation =', 3) ;  -2.01500907
match ; Kinetic energy     ; GREPFIELD(static/info, 'Kinetic     =', 3) ;   184.83544058
match ; External energy    ; GREPFIELD(static/info, 'External    =', 3) ;  -344.67227002
match ; Hubbard energy     ; GREPFIELD(static/info, 'Hubbard     =', 3) ;   0.13682370
match ; Total Magnetic Moment ; GREPFIELD(static/info, 'mz = ', 3);  0.000000    
match ; Local Magnetic Moment (Ni1) ; GREPFIELD(static/info, '1        Ni', 3);  1.880931
match ; Local Magnetic Moment (Ni2) ; GREPFIELD(static/info, '2        Ni', 3); -1.880931
match ; Local Magnetic Moment (O1) ;  GREPFIELD(static/info, '3         O', 3);  0.0000
match ; Local Magnetic Moment (O2) ;  GREPFIELD(static/info, '4         O', 3);  0.0000

match ; k-point 1 (x) ; GREPFIELD(static/info, '#k =   1', 7) ; 0.000000 
match ; k-point 1 (y) ; GREPFIELD(static/info, '#k =   1', 8) ; 0.000000
match ; k-point 1 (z) ; GREPFIELD(static/info, '#k =   1', 9) ; 0.000000
match ; Eigenvalue  1 up ; GREPFIELD(static/info, '#k =   1', 3, 1) ;  -1.949645
match ; Eigenvalue  4 dn ; GREPFIELD(static/info, '#k =   1', 3, 8) ;  -1.853844 
match ; Eigenvalue  8 dn ; GREPFIELD(static/info, '#k =   1', 3, 16) ; -0.779770
match ; Eigenvalue  9 up ; GREPFIELD(static/info, '#k =   1', 3, 17) ; -0.778069

match ; Occupation Ni2 down 3d4 ; LINEFIELD(static/occ_matrices, -2, 7) ; 0.97924163
match ; Occupation Ni2 down 3d5 ; LINEFIELD(static/occ_matrices, -1, 9) ; 0.99547864

match ; Ueff 3d Ni1 ; LINEFIELD(static/effectiveU, -10, 4) ; 0.560342
match ; Ueff 3d Ni2 ; LINEFIELD(static/effectiveU, -9, 4) ; 0.560342
match ; U 3d Ni1    ; LINEFIELD(static/effectiveU, -6, 4) ; 0.830145
match ; U 3d Ni2    ; LINEFIELD(static/effectiveU, -5, 4) ; 0.830145
match ; J 3d Ni1    ; LINEFIELD(static/effectiveU, -2, 4) ; 0.269803
match ; J 3d Ni2    ; LINEFIELD(static/effectiveU, -1, 4) ; 0.269803

match ; Kanamori U Ni1  ; LINEFIELD(static/kanamoriU, -10, 4) ; 0.858229
match ; Kanamori U Ni2  ; LINEFIELD(static/kanamoriU, -9, 4) ; 0.858229
match ; Kanamori Up Ni1 ; LINEFIELD(static/kanamoriU, -6, 4) ; 0.687113
match ; Kanamori Up Ni2 ; LINEFIELD(static/kanamoriU, -5, 4) ; 0.687113
match ; Kanamori J Ni1  ; LINEFIELD(static/kanamoriU, -2, 4) ; 0.026773
match ; Kanamori J Ni2  ; LINEFIELD(static/kanamoriU, -1, 4) ; 0.026773

Processors : 4
Precision : 0.0001

Input      : 02-ACBN0.02-lif.inp

match ; SCF convergence ; GREPCOUNT(static/info, 'SCF converged') ; 1
match ; Total energy       ; GREPFIELD(static/info, 'Total       =', 3) ;  -24.27497649
match ; Ion-ion energy     ; GREPFIELD(static/info, 'Ion-ion     =', 3) ;  -14.42250723
match ; Eigenvalues sum    ; GREPFIELD(static/info, 'Eigenvalues =', 3) ;  -4.48871744
match ; Hartree energy     ; GREPFIELD(static/info, 'Hartree     =', 3) ;   8.24513305
match ; Exchange energy    ; GREPFIELD(static/info, 'Exchange    =', 3) ;  -4.36714638
match ; Correlation energy ; GREPFIELD(static/info, 'Correlation =', 3) ;  -0.49296712
match ; Kinetic energy     ; GREPFIELD(static/info, 'Kinetic     =', 3) ;   17.46606874
match ; External energy    ; GREPFIELD(static/info, 'External    =', 3) ;  -30.75881834
match ; Hubbard energy     ; GREPFIELD(static/info, 'Hubbard     =', 3) ;   0.05530345
match ; Total Magnetic Moment ; GREPFIELD(static/info, 'mz = ', 3);  0.000000
match ; Local Magnetic Moment (Li) ; GREPFIELD(static/info, '1        Li', 3);  0.000000
match ; Local Magnetic Moment (F)  ; GREPFIELD(static/info, '2         F', 3);  0.000000

match ; k-point 2 (x) ; GREPFIELD(static/info, '#k =   2', 7) ;  0.000000
match ; k-point 2 (y) ; GREPFIELD(static/info, '#k =   2', 8) ;  0.500000
match ; k-point 2 (z) ; GREPFIELD(static/info, '#k =   2', 9) ;  0.000000
match ; Eigenvalue  1 up ; GREPFIELD(static/info, '#k =   2', 3, 1) ;  -0.943917
match ; Eigenvalue  1 dn ; GREPFIELD(static/info, '#k =   2', 3, 2) ;  -0.943917
match ; Eigenvalue  3 up ; GREPFIELD(static/info, '#k =   2', 3, 5) ;  -0.408362
match ; Eigenvalue  4 up ; GREPFIELD(static/info, '#k =   2', 3, 7) ;  -0.406175
match ; Eigenvalue  5 up ; GREPFIELD(static/info, '#k =   2', 3, 9) ;   0.284744

match ; Occupation F down 2p2 ; LINEFIELD(static/occ_matrices, -2, 3) ; 0.96106613
match ; Occupation F down 2p3 ; LINEFIELD(static/occ_matrices, -1, 5) ; 0.96106613

match ; U2p F   ; LINEFIELD(static/effectiveU, -7, 4) ; 0.492650

match ; Kanamori U   ; LINEFIELD(static/kanamoriU, -7, 4) ; 0.857185
match ; Kanamori Up  ; LINEFIELD(static/kanamoriU, -4, 4) ; 0.775257
match ; Kanamori J   ; LINEFIELD(static/kanamoriU, -1, 4) ; 0.041837
