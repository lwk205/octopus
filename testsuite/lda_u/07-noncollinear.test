# -*- coding: utf-8 mode: shell-script -*-

Test       : GGA+U for (AF) bulk NiO crystal with noncollinear spins
Program    : octopus
TestGroups : long-run, periodic_systems, lda_u
Enabled    : Yes

Processors : 2

Input      : 07-noncollinear.01-U5-gs.inp

match ; SCF convergence ; GREPCOUNT(static/info, 'SCF converged') ; 1

match ; Total k-points   ; GREPFIELD(static/info, 'Total number of k-points', 6) ; 4
match ; Reduced k-points ; GREPFIELD(static/info, 'Number of symmetry-reduced k-points', 6) ; 2

match ;  SCF convergence  ; GREPCOUNT(static/info, 'SCF converged') ; 1.0
match ;  Total k-points    ; GREPFIELD(static/info, 'Total number of k-points', 6) ; 4.0
match ;  Reduced k-points  ; GREPFIELD(static/info, 'Number of symmetry-reduced k-points', 6) ; 2.0

Precision: 2.8e-3
match ;   Total energy         ; GREPFIELD(static/info, 'Total       =', 3) ; -246.702704295
match ;   Ion-ion energy       ; GREPFIELD(static/info, 'Ion-ion     =', 3) ; -177.00987193
Precision: 2e-3
match ;   Eigenvalues sum      ; GREPFIELD(static/info, 'Eigenvalues =', 3) ; -33.90311739
Precision: 8e-4
match ;   Hartree energy       ; GREPFIELD(static/info, 'Hartree     =', 3) ;  46.014508605
Precision: 1.2e-04
match ;   Exchange energy      ; GREPFIELD(static/info, 'Exchange    =', 3) ; -26.378348245
Precision: 2.2e-5
match ;   Correlation energy   ; GREPFIELD(static/info, 'Correlation =', 3) ; -2.75275979
Precision: 1.3e-3
match ;   Kinetic energy       ; GREPFIELD(static/info, 'Kinetic     =', 3) ; 122.95380328
Precision: 8.4e-4
match ;   External energy      ; GREPFIELD(static/info, 'External    =', 3) ; -209.82259783
Precision: 4.1e-05
match ;   Hubbard energy       ; GREPFIELD(static/info, 'Hubbard     =', 3) ; 0.29122356

Precision: 1e-15
match ;   Total Magnetic Moment x   ; GREPFIELD(static/info, 'mx = ', 3) ; 0.0
match ;   Total Magnetic Moment y   ; GREPFIELD(static/info, 'my = ', 3) ; 0.0
match ;   Total Magnetic Moment z   ; GREPFIELD(static/info, 'mz = ', 3) ; 0.0

Precision: 4.8e-04
match ;   Local Magnetic Moment (Ni1)   ; GREPFIELD(static/info, '1        Ni', 3) ;  3.3317715
match ;   Local Magnetic Moment (Ni2)   ; GREPFIELD(static/info, '2        Ni', 3) ; -3.3317715

Precision: 1e-15
match ;   Local Magnetic Moment (O1)   ; GREPFIELD(static/info, '3         O', 3) ; 0.0
match ;   Local Magnetic Moment (O2)   ; GREPFIELD(static/info, '4         O', 3) ; 0.0

match ;   k-point 1 (x)   ; GREPFIELD(static/info, '#k =   1', 7) ; 0.0
match ;   k-point 1 (y)   ; GREPFIELD(static/info, '#k =   1', 8) ; 0.0
match ;   k-point 1 (z)   ; GREPFIELD(static/info, '#k =   1', 9) ; 0.0

Precision: 1.3e-4
match ;   Eigenvalue  1   ; GREPFIELD(static/info, '#k =   1', 3, 1) ; -3.06518
Precision: 6.4e-05
match ;   Eigenvalue  8   ; GREPFIELD(static/info, '#k =   1', 3, 8) ; -2.9364925
match ;   Eigenvalue 16   ; GREPFIELD(static/info, '#k =   1', 3, 16) ; -0.4583535
match ;   Eigenvalue 17   ; GREPFIELD(static/info, '#k =   1', 3, 17) ; -0.1837930

Precision: 7.1e-4
match ;   Occupation Ni2 up-down 3d4   ; LINEFIELD(static/occ_matrices, -2, 7) ; -0.04498016
match ;   Occupation Ni2 up-down 3d5   ; LINEFIELD(static/occ_matrices, -1, 9) ; -0.391949885



