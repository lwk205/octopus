# -*- coding: utf-8 mode: shell-script -*-

Test       : Diamond TB09 functional
Program    : octopus
TestGroups : long-run, periodic_systems
Enabled    : Yes

# ground state
Input      : 07-tb09.01-gs_symm.inp

#The reference values for this test must be copied from the one without symmetries
match ; Total k-points   ; GREPFIELD(static/info, 'Total number of k-points', 6) ; 27
match ; Reduced k-points ; GREPFIELD(static/info, 'Number of symmetry-reduced k-points', 6) ; 4
match ; Space group        ; GREPFIELD(out, 'Space group', 4) ; 227
match ; No. of symmetries  ; GREPFIELD(out, 'symmetries that can be used', 5)  ;  24

match ; SCF convergence    ; GREPCOUNT(static/info, 'SCF converged') ; 1
match ; Total energy       ; GREPFIELD(static/info, 'Total       =', 3) ; -33.67787322
match ; Ion-ion energy     ; GREPFIELD(static/info, 'Ion-ion     =', 3) ; -42.94217323
match ; Eigenvalues sum    ; GREPFIELD(static/info, 'Eigenvalues =', 3) ; -3.63611007
match ; Hartree energy     ; GREPFIELD(static/info, 'Hartree     =', 3) ; 3.86210883
match ; Exchange energy    ; GREPFIELD(static/info, 'Exchange    =', 3) ; 0.00000000
match ; Correlation energy ; GREPFIELD(static/info, 'Correlation =', 3) ; -1.77973455
match ; Kinetic energy     ; GREPFIELD(static/info, 'Kinetic     =', 3) ; 33.05361881
match ; External energy    ; GREPFIELD(static/info, 'External    =', 3) ; -25.87169151

match ; Eigenvalue [  k=1, n=1   ] ; GREPFIELD(static/info, '#k =   1', 3, 1) ; -0.548684
match ; Eigenvalue [  k=1, n=8   ] ; GREPFIELD(static/info, '#k =   1', 3, 8) ; -0.031869
match ; Eigenvalue [  k=1, n=16  ] ; GREPFIELD(static/info, '#k =   1', 3, 16) ; 0.194339
match ; Eigenvalue [  k=1, n=17  ] ; GREPFIELD(static/info, '#k =   1', 3, 17) ; 0.433326

match ; Eigenvalue [  k=2, n=1   ] ; GREPFIELD(static/info, '#k =   2', 3, 1) ; -0.512066
match ; Eigenvalue [  k=2, n=8   ] ; GREPFIELD(static/info, '#k =   2', 3, 8) ; -0.080937
match ; Eigenvalue [  k=2, n=16  ] ; GREPFIELD(static/info, '#k =   2', 3, 16) ; 0.125477
match ; Eigenvalue [  k=2, n=17  ] ; GREPFIELD(static/info, '#k =   2', 3, 17) ; 0.407856

match ; Direct gap ; GREPFIELD(static/info, 'Direct gap', 7, 0) ; 0.2390
match ; Indirect gap ; GREPFIELD(static/info, 'Indirect gap', 10, 0) ; 0.2135

#No check for k-points higher than 2, as they might not belong to the IBZ

Input      : 07-tb09.02-gs_nosymm.inp

