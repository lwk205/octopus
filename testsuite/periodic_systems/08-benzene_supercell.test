# -*- coding: utf-8 mode: shell-script -*-

Test       : Benzene supercell
Program    : octopus
TestGroups : short-run, periodic_systems
Enabled    : Yes

Processors: 4
Input: 08-benzene_supercell.01-gs.inp

match ; Total k-points   ; GREPFIELD(static/info, 'Total number of k-points', 6) ; 1
match ; Reduced k-points ; GREPFIELD(static/info, 'Number of symmetry-reduced k-points', 6) ; 1
match ; Space group        ; GREPFIELD(out, 'Space group', 4) ; 47
match ; No. of symmetries  ; GREPFIELD(out, 'symmetries that can be used', 5)  ;   8

match ; SCF convergence ; GREPCOUNT(static/info, 'SCF converged') ; 1

match ; Dipole x     ; GREPFIELD(static/info, '<x>', 3)       ;   0.0
Precision: 0.00016
match ; Dipole y     ; GREPFIELD(static/info, '<y>', 3)       ;   0.0
Precision: 0.0001
match ; Dipole z     ; GREPFIELD(static/info, '<z>', 3)       ;   0.0

match ; Total energy       ; GREPFIELD(static/info, 'Total       =', 3) ; -37.05476848
match ; Eigenvalues sum    ; GREPFIELD(static/info, 'Eigenvalues =', 3) ; -10.89877154
match ; Hartree energy     ; GREPFIELD(static/info, 'Hartree     =', 3) ;  31.45028706
match ; Exchange energy    ; GREPFIELD(static/info, 'Exchange    =', 3) ; -11.17138460
match ; Correlation energy ; GREPFIELD(static/info, 'Correlation =', 3) ; -1.62627212
match ; Kinetic energy     ; GREPFIELD(static/info, 'Kinetic     =', 3) ;  30.07139976
match ; External energy    ; GREPFIELD(static/info, 'External    =', 3) ; -87.13850264

match ; Eigenvalue  1      ; GREPFIELD(static/info, ' 1   --', 3) ; -0.751362
match ; Eigenvalue  2      ; GREPFIELD(static/info, ' 2   --', 3) ; -0.643005
match ; Eigenvalue  3      ; GREPFIELD(static/info, ' 3   --', 3) ; -0.564351
match ; Eigenvalue  4      ; GREPFIELD(static/info, ' 4   --', 3) ; -0.557417
match ; Eigenvalue  5      ; GREPFIELD(static/info, ' 5   --', 3) ; -0.486867
match ; Eigenvalue  6      ; GREPFIELD(static/info, ' 6   --', 3) ; -0.423139
match ; Eigenvalue  7      ; GREPFIELD(static/info, ' 7   --', 3) ; -0.403333
match ; Eigenvalue  8      ; GREPFIELD(static/info, ' 8   --', 3) ; -0.315952
match ; Eigenvalue  9      ; GREPFIELD(static/info, ' 9   --', 3) ; -0.313512
match ; Eigenvalue 10      ; GREPFIELD(static/info, '10   --', 3) ; -0.254369
match ; Eigenvalue 11      ; GREPFIELD(static/info, '11   --', 3) ; -0.214466
match ; Eigenvalue 12      ; GREPFIELD(static/info, '12   --', 3) ; -0.135060
match ; Eigenvalue 13      ; GREPFIELD(static/info, '13   --', 3) ; -0.133878
match ; Eigenvalue 14      ; GREPFIELD(static/info, '14   --', 3) ; -0.133286
match ; Eigenvalue 15      ; GREPFIELD(static/info, '15   --', 3) ; -0.119389
match ; Eigenvalue 16      ; GREPFIELD(static/info, '16   --', 3) ;  0.004535
Precision: 0.007
match ; Eigenvalue 17      ; GREPFIELD(static/info, '17   --', 3) ;  0.059968
Precision: 0.005
match ; Eigenvalue 18      ; GREPFIELD(static/info, '18   --', 3) ;  0.064097
