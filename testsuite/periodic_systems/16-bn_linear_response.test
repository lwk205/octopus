# -*- coding: utf-8 mode: shell-script -*-

Test       : Magneto-Optics and Magnetic Susceptibility for Solids
Program    : octopus
TestGroups : long-run, periodic systems
Enabled    : Yes

Input: 16-bn_linear_response.01-gs.inp
match ; Total energy ; GREPFIELD(static/info, 'Total       =', 3) ; -30.25187062
match ; Eigenvalues  ; GREPFIELD(static/info, '1   --', 3) ; -1.983219

Input: 16-bn_linear_response.02-kdotp.inp
Precision : 0.001
match ; Inverse eff mass    ; GREPFIELD(kdotp/kpoint_1_1, "State #4",  1,  1) ;  0.454689
match ; Inverse eff mass    ; GREPFIELD(kdotp/kpoint_4_1, "State #8",  2,  2) ;  -3.629786

Input: 16-bn_linear_response.03-susc.inp
Precision : 0.1
match ; Susceptibility XX          ; GREPFIELD(em_resp/freq_0.0000/susceptibility, "susceptibility", 1, 1) ; 233.988448
match ; Susceptibility YY          ; GREPFIELD(em_resp/freq_0.0000/susceptibility, "susceptibility", 2, 2) ; 261.229929
Precision : 3
match ; Susceptibility ZZ          ; GREPFIELD(em_resp/freq_0.0000/susceptibility, "susceptibility", 3, 3) ; 20994.325096

Input: 16-bn_linear_response.04-lfe_susc.inp
Precision : 0.1
match ; Susceptibility XX          ; GREPFIELD(em_resp/freq_0.0000/susceptibility, "susceptibility", 1, 1) ; 233.988221
match ; Susceptibility YY          ; GREPFIELD(em_resp/freq_0.0000/susceptibility, "susceptibility", 2, 2) ; 261.229920
Precision : 3
match ; Susceptibility ZZ          ; GREPFIELD(em_resp/freq_0.0000/susceptibility, "susceptibility", 3, 3) ; 20994.324948

Input: 16-bn_linear_response.05-pol_magn_opt.inp
Precision : 0.1
match ; Susceptibility XX          ; GREPFIELD(em_resp/freq_0.0000/susceptibility, "susceptibility", 1, 1) ; 233.988462
match ; Susceptibility YY          ; GREPFIELD(em_resp/freq_0.0000/susceptibility, "susceptibility", 2, 2) ; 261.229920
Precision : 3
match ; Susceptibility ZZ          ; GREPFIELD(em_resp/freq_0.0000/susceptibility, "susceptibility", 3, 3) ; 20994.324303

Precision : 0.001
match ; Re dielectric constant YY  ; GREPFIELD(em_resp/freq_0.2000/epsilon, "Real", 2, 2) ; -1.013191
match ; Re dielectric constant ZZ  ; GREPFIELD(em_resp/freq_0.2000/epsilon, "Real", 3, 3) ; 1.576969
match ; Im dielectric constant XX  ; GREPFIELD(em_resp/freq_0.2000/epsilon, "Imaginary", 1, 1) ; 0.372312
match ; Im dielectric constant av  ; GREPFIELD(em_resp/freq_0.2000/epsilon, "Imaginary", 3, 4) ; 0.484057

Precision : 0.003
match ; Polarizability XX          ; GREPFIELD(em_resp/freq_0.2000/alpha, "Polarizability", 1, 1) ; -122.237367
match ; Polarizability av          ; GREPFIELD(em_resp/freq_0.2000/alpha, "Polarizability", 3, 4) ; -98.679350

Precision : 0.001
match ; Re epsilon at k-point      ; GREPFIELD(em_resp/freq_0.2000/epsilon_k_re, "index", 14, 1) ; 0.55854968E+00
match ; Re epsilon at k-point      ; GREPFIELD(em_resp/freq_0.2000/epsilon_k_re, "index", 10, 3) ; -0.53261875E+01
match ; Im epsilon at k-point      ; GREPFIELD(em_resp/freq_0.2000/epsilon_k_im, "index", 10, 2) ; 0.83029258E+00
match ; Im epsilon at k-point      ; GREPFIELD(em_resp/freq_0.2000/epsilon_k_im, "index", 6, 4) ; 0.29231539E+00

Precision : 0.003
match ; Mag.-opt. Re alpha         ; GREPFIELD(em_resp/freq_0.2000/alpha_mo, 'Re alpha', 5) ; 0.30603741E+00
match ; Mag.-opt. Im alpha         ; GREPFIELD(em_resp/freq_0.2000/alpha_mo, 'Im alpha', 6) ; 0.83059648E+01
Precision : 0.00001
match ; Mag.-opt. Re eps           ; GREPFIELD(em_resp/freq_0.2000/alpha_mo, 'Re epsilon', 7) ; -0.79702989E-02
match ; Mag.-opt. av Im eps        ; GREPFIELD(em_resp/freq_0.2000/alpha_mo, 'Im epsilon', 10) ; 0.94894607E-02

match ; Mag.-opt. Re eps at kpt    ; GREPFIELD(em_resp/freq_0.2000/epsilon_mo_k, "index", 6, 1) ; 0.25290560E-02
match ; Mag.-opt. Re eps at kpt    ; GREPFIELD(em_resp/freq_0.2000/epsilon_mo_k, "index", 8, 4) ; 0.56698163E-02
match ; Mag.-opt. Im eps at kpt    ; GREPFIELD(em_resp/freq_0.2000/epsilon_mo_k, "index", 11, 2) ; 0.31733483E-01

Input: 16-bn_linear_response.06-lfe_magn_opt.inp
Precision : 0.1
match ; Susceptibility XX          ; GREPFIELD(em_resp/freq_0.0000/susceptibility, "susceptibility", 1, 1) ; 233.988241
match ; Susceptibility YY          ; GREPFIELD(em_resp/freq_0.0000/susceptibility, "susceptibility", 2, 2) ; 261.229916
Precision : 3
match ; Susceptibility ZZ          ; GREPFIELD(em_resp/freq_0.0000/susceptibility, "susceptibility", 3, 3) ; 20994.324967

Precision : 0.001
match ; Re dielectric constant XX  ; GREPFIELD(em_resp/freq_0.2000/epsilon, "Real", 1, 1) ; -0.034169
match ; Re dielectric constant ZZ  ; GREPFIELD(em_resp/freq_0.2000/epsilon, "Real", 3, 3) ; 1.158435
match ; Im dielectric constant YY  ; GREPFIELD(em_resp/freq_0.2000/epsilon, "Imaginary", 2, 2) ; 0.990997
match ; Im dielectric constant av  ; GREPFIELD(em_resp/freq_0.2000/epsilon, "Imaginary", 3, 4) ; 0.453538

Precision : 0.003
match ; Polarizability YY          ; GREPFIELD(em_resp/freq_0.2000/alpha, "Polarizability", 1, 1) ; -125.147252
match ; Polarizability ZZ          ; GREPFIELD(em_resp/freq_0.2000/alpha, "Polarizability", 3, 3) ; 19.172616
match ; Polarizability av          ; GREPFIELD(em_resp/freq_0.2000/alpha, "Polarizability", 3, 4) ; -120.969062

Precision : 0.0003
match ; Mag.-opt. Re alpha         ; GREPFIELD(em_resp/freq_0.2000/alpha_mo, 'Re alpha', 4) ; -0.33317828E+00
match ; Mag.-opt. Im alpha         ; GREPFIELD(em_resp/freq_0.2000/alpha_mo, 'Im alpha', 6) ; 0.98655100E+01

Precision : 0.00001
match ; Mag.-opt. Re eps           ; GREPFIELD(em_resp/freq_0.2000/alpha_mo, 'Re epsilon', 7) ; -0.27532569E-02
match ; Mag.-opt. av Im eps        ; GREPFIELD(em_resp/freq_0.2000/alpha_mo, 'Im epsilon', 10) ; 0.17125820E-01


