# -*- coding: utf-8 mode: shell-script -*-

Test       : Magneto-Optics and Magnetic Susceptibility for Solids
Program    : octopus
TestGroups : long-run, periodic systems
Enabled    : Yes

Input: 16-bn_linear_response.01-gs.inp
match ; Total energy ; GREPFIELD(static/info, 'Total       =', 3) ; -30.25187062
match ; Eigenvalues  ; GREPFIELD(static/info, '1   --', 3) ; -1.983219

Input: 16-bn_linear_response.02-kdotp.inp
Precision : 0.001
match ; Inverse eff mass    ; GREPFIELD(kdotp/kpoint_1_1, "State #4",  1,  1) ;  0.454689
match ; Inverse eff mass    ; GREPFIELD(kdotp/kpoint_4_1, "State #8",  2,  2) ;  -3.629786

Input: 16-bn_linear_response.03-susc.inp
Precision : 0.001
match ; Susceptibility XX          ; LINEFIELD(em_resp/freq_0.0000/susceptibility, 2, 1) ; 233.988448
match ; Susceptibility YY          ; LINEFIELD(em_resp/freq_0.0000/susceptibility, 3, 2) ; 261.229929
match ; Susceptibility ZZ          ; LINEFIELD(em_resp/freq_0.0000/susceptibility, 4, 3) ; 20994.325096

Input: 16-bn_linear_response.04-lfe_susc.inp
Precision : 0.001
match ; Susceptibility XX          ; LINEFIELD(em_resp/freq_0.0000/susceptibility, 2, 1) ; 233.988448
match ; Susceptibility YY          ; LINEFIELD(em_resp/freq_0.0000/susceptibility, 3, 2) ; 261.229929
match ; Susceptibility ZZ          ; LINEFIELD(em_resp/freq_0.0000/susceptibility, 4, 3) ; 20994.325096

Input: 16-bn_linear_response.05-pol_magn_opt.inp
Precision : 0.001
match ; Susceptibility XX          ; LINEFIELD(em_resp/freq_0.0000/susceptibility, 2, 1) ; 233.988462
match ; Susceptibility YY          ; LINEFIELD(em_resp/freq_0.0000/susceptibility, 3, 2) ; 261.231258
match ; Susceptibility ZZ          ; LINEFIELD(em_resp/freq_0.0000/susceptibility, 4, 3) ; 20994.324303

match ; Re dielectric constant YY  ; LINEFIELD(em_resp/freq_0.2000/epsilon, 3, 2) ; -1.013171
match ; Re dielectric constant ZZ  ; LINEFIELD(em_resp/freq_0.2000/epsilon, 4, 3) ; 1.576967
match ; Im dielectric constant XX  ; LINEFIELD(em_resp/freq_0.2000/epsilon, 8, 1) ; 0.372283
match ; Im dielectric constant av  ; LINEFIELD(em_resp/freq_0.2000/epsilon, 11, 3) ; 0.484063

match ; Polarizability XX          ; LINEFIELD(em_resp/freq_0.2000/alpha, 2, 1) ; -122.238321
match ; Polarizability av          ; LINEFIELD(em_resp/freq_0.2000/alpha, 5, 3) ; -98.678932

match ; Re epsilon at k-point      ; LINEFIELD(em_resp/freq_0.2000/epsilon_k_re, 3, 14) ; 0.55851875E+00
match ; Re epsilon at k-point      ; LINEFIELD(em_resp/freq_0.2000/epsilon_k_re, 5, 10) ; -0.53262570E+01
match ; Im epsilon at k-point      ; LINEFIELD(em_resp/freq_0.2000/epsilon_k_im, 4, 10) ; 0.83029798E+00
match ; Im epsilon at k-point      ; LINEFIELD(em_resp/freq_0.2000/epsilon_k_im, 6, 6) ; 0.29229845E+00

Precision : 0.00001
match ; Mag.-opt. Re alpha         ; GREPFIELD(em_resp/freq_0.2000/alpha_mo, 'Re alpha', 5) ; 0.30521148E+00
match ; Mag.-opt. Im alpha         ; GREPFIELD(em_resp/freq_0.2000/alpha_mo, 'Im alpha', 6) ; 0.83050827E+01
match ; Mag.-opt. Re eps           ; GREPFIELD(em_resp/freq_0.2000/alpha_mo, 'Re epsilon', 7) ; -0.79679781E-02
match ; Mag.-opt. av Im eps        ; GREPFIELD(em_resp/freq_0.2000/alpha_mo, 'Im epsilon', 10) ; 0.94868399E-02

match ; Mag.-opt. Re eps at kpt    ; LINEFIELD(em_resp/freq_0.2000/epsilon_mo_k, 3, 6) ; 0.25256908E-02
match ; Mag.-opt. Re eps at kpt    ; LINEFIELD(em_resp/freq_0.2000/epsilon_mo_k, 6, 8) ; 0.56744593E-02
match ; Mag.-opt. Im eps at kpt    ; LINEFIELD(em_resp/freq_0.2000/epsilon_mo_k, 4, 11) ; 0.31731659E-01

Input: 16-bn_linear_response.06-lfe_magn_opt.inp
Precision : 0.001
match ; Susceptibility XX          ; LINEFIELD(em_resp/freq_0.0000/susceptibility, 2, 1) ; 233.988687
match ; Susceptibility YY          ; LINEFIELD(em_resp/freq_0.0000/susceptibility, 3, 2) ; 261.231297
match ; Susceptibility ZZ          ; LINEFIELD(em_resp/freq_0.0000/susceptibility, 4, 3) ; 20994.324270

match ; Re dielectric constant XX  ; LINEFIELD(em_resp/freq_0.2000/epsilon, 2, 1) ; -0.034160
match ; Re dielectric constant ZZ  ; LINEFIELD(em_resp/freq_0.2000/epsilon, 4, 3) ; 1.158435
match ; Im dielectric constant YY  ; LINEFIELD(em_resp/freq_0.2000/epsilon, 9, 2) ; 0.991176
match ; Im dielectric constant av  ; LINEFIELD(em_resp/freq_0.2000/epsilon, 11, 3) ; 0.453585

match ; Polarizability YY          ; LINEFIELD(em_resp/freq_0.2000/alpha, 2, 1) ; -125.146168
match ; Polarizability ZZ          ; LINEFIELD(em_resp/freq_0.2000/alpha, 4, 3) ; 19.172567
match ; Polarizability av          ; LINEFIELD(em_resp/freq_0.2000/alpha, 5, 3) ; -120.979302

Precision : 0.00001
match ; Mag.-opt. Re alpha         ; GREPFIELD(em_resp/freq_0.2000/alpha_mo, 'Re alpha', 4) ; -0.33322222E+00
match ; Mag.-opt. Im alpha         ; GREPFIELD(em_resp/freq_0.2000/alpha_mo, 'Im alpha', 6) ; 0.98661259E+01
match ; Mag.-opt. Re eps           ; GREPFIELD(em_resp/freq_0.2000/alpha_mo, 'Re epsilon', 7) ; -0.27531286E-02
match ; Mag.-opt. av Im eps        ; GREPFIELD(em_resp/freq_0.2000/alpha_mo, 'Im epsilon', 10) ; 0.17125669E-01


