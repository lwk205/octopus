# -*- coding: utf-8 mode: shell-script -*-

Test       : Real space symmetrization with GGA
Program    : octopus
TestGroups : long-run, periodic_systems, symmetries
Enabled    : Yes

#Here the reference values should be copied from the calculation without symmetries
Input : 09-symmetrization_gga.01-spg143_sym.inp

Precision: 0.0005
match ; Total energy       ; GREPFIELD(static/info, 'Total       =', 3) ; -0.14445092
Precision: 0.0001
match ; Ion-ion energy     ; GREPFIELD(static/info, 'Ion-ion     =', 3) ; -0.13210182
match ; Eigenvalues sum    ; GREPFIELD(static/info, 'Eigenvalues =', 3) ; -0.13735994
Precision: 0.0006
match ; Hartree energy     ; GREPFIELD(static/info, 'Hartree     =', 3) ;  0.03323673
match ; Exchange energy    ; GREPFIELD(static/info, 'Exchange    =', 3) ; -0.43967437
Precision: 0.0001
match ; Correlation energy ; GREPFIELD(static/info, 'Correlation =', 3) ; -0.09064816
Precision: 0.0008
match ; Kinetic energy     ; GREPFIELD(static/info, 'Kinetic     =', 3) ;  0.37645690
match ; External energy    ; GREPFIELD(static/info, 'External    =', 3) ;  0.11107048
Precision: 0.0001

match ; Eigenvalue [  k=1, n=1   ] ; GREPFIELD(static/info, '#k =   1', 3, 1) ; -0.137450
match ; Eigenvalue [  k=1, n=2   ] ; GREPFIELD(static/info, '#k =   1', 3, 2) ;  0.072843
match ; Eigenvalue [  k=1, n=3   ] ; GREPFIELD(static/info, '#k =   1', 3, 3) ;  0.136544

match ; Eigenvalue [  k=2, n=1   ] ; GREPFIELD(static/info, '#k =   2', 3, 1) ; -0.112304
match ; Eigenvalue [  k=2, n=2   ] ; GREPFIELD(static/info, '#k =   2', 3, 2) ;  0.084363
match ; Eigenvalue [  k=2, n=3   ] ; GREPFIELD(static/info, '#k =   2', 3, 3) ;  0.097229

match ; Force 1 (x)   ; GREPFIELD(static/info, '1        Na', 3) ;  0.171666
match ; Force 1 (y)   ; GREPFIELD(static/info, '1        Na', 4) ;  0.236046
match ; Force 1 (z)   ; GREPFIELD(static/info, '1        Na', 5) ; -0.0
match ; Force 2 (x)   ; GREPFIELD(static/info, '2        Na', 3) ;  0.118585
match ; Force 2 (y)   ; GREPFIELD(static/info, '2        Na', 4) ; -0.266654
match ; Force 2 (z)   ; GREPFIELD(static/info, '2        Na', 5) ;  0.0
match ; Force 3 (x)   ; GREPFIELD(static/info, '3        Na', 3) ; -0.290247
match ; Force 3 (y)   ; GREPFIELD(static/info, '3        Na', 4) ;  0.030615
match ; Force 3 (z)   ; GREPFIELD(static/info, '3        Na', 5) ;  0.0


match ; Partial charge  1 ; GREPFIELD(static/info, 'Partial ionic charges', 3, 2)  ; 1.0
match ; Partial charge  2 ; GREPFIELD(static/info, 'Partial ionic charges', 3, 3)  ; 1.0
match ; Density value 1   ; LINEFIELD(static/density.y=0\,z=0,  2, 2)   ; 9.85422937135914E-003
match ; Density value 2   ; LINEFIELD(static/density.y=0\,z=0,  3, 2)   ; 8.75599886929163E-003
match ; Bader value 1     ; LINEFIELD(static/bader.y=0\,z=0,  6, 2) ; 9.94863552340998E-003
match ; Bader value 2     ; LINEFIELD(static/bader.y=0\,z=0, 10, 2) ; 1.15040762637793E-002
