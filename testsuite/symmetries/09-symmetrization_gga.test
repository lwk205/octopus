# -*- coding: utf-8 mode: shell-script -*-

Test       : Real space symmetrization with GGA
Program    : octopus
TestGroups : long-run, periodic_systems, symmetries
Enabled    : Yes

Input : 09-symmetrization_gga.01-spg143_sym.inp

match ;  SCF convergence  ; GREPCOUNT(static/info, 'SCF converged') ; 1.0
Precision: 1e-6
match ;  Total energy        ; GREPFIELD(static/info, 'Total       =', 3) ; -0.14193536
match ;  Ion-ion energy      ; GREPFIELD(static/info, 'Ion-ion     =', 3) ; -0.13210182
match ;  Eigenvalues sum     ; GREPFIELD(static/info, 'Eigenvalues =', 3) ; -0.13691364
match ;  Hartree energy      ; GREPFIELD(static/info, 'Hartree     =', 3) ; 0.03088307
match ;  Exchange energy     ; GREPFIELD(static/info, 'Exchange    =', 3) ; -0.43851819
match ;  Correlation energy  ; GREPFIELD(static/info, 'Correlation =', 3) ; -0.09081481
match ;  Kinetic energy      ; GREPFIELD(static/info, 'Kinetic     =', 3) ; 0.37446427
match ;  External energy     ; GREPFIELD(static/info, 'External    =', 3) ; 0.1141429

Precision: 1e-6
match ;  Eigenvalue [  k=1, n=1   ]  ; GREPFIELD(static/info, '#k =   1', 3, 1) ; -0.137287
match ;  Eigenvalue [  k=1, n=2   ]  ; GREPFIELD(static/info, '#k =   1', 3, 2) ; 0.072766
match ;  Eigenvalue [  k=1, n=3   ]  ; GREPFIELD(static/info, '#k =   1', 3, 3) ; 0.136578
match ;  Eigenvalue [  k=2, n=1   ]  ; GREPFIELD(static/info, '#k =   2', 3, 1) ; -0.112137
match ;  Eigenvalue [  k=2, n=2   ]  ; GREPFIELD(static/info, '#k =   2', 3, 2) ; 0.084509
match ;  Eigenvalue [  k=2, n=3   ]  ; GREPFIELD(static/info, '#k =   2', 3, 3) ; 0.097145

Precision: 1e-8
match ;  Force 1 (x)    ; GREPFIELD(static/info, '1        Na', 3) ; 0.171571
match ;  Force 1 (y)    ; GREPFIELD(static/info, '1        Na', 4) ; 0.235936
match ;  Force 1 (z)    ; GREPFIELD(static/info, '1        Na', 5) ; 9.79254e-06
match ;  Force 2 (x)    ; GREPFIELD(static/info, '2        Na', 3) ; 0.118541
match ;  Force 2 (y)    ; GREPFIELD(static/info, '2        Na', 4) ; -0.266553
match ;  Force 2 (z)    ; GREPFIELD(static/info, '2        Na', 5) ; 9.75673e-06
match ;  Force 3 (x)    ; GREPFIELD(static/info, '3        Na', 3) ; -0.290112
match ;  Force 3 (y)    ; GREPFIELD(static/info, '3        Na', 4) ; 0.0306168
match ;  Force 3 (z)    ; GREPFIELD(static/info, '3        Na', 5) ; 9.73192e-06

Precision: 1e-8
match ;  Partial charge  1  ; GREPFIELD(static/info, 'Partial ionic charges', 3, 2) ; 1.0
match ;  Partial charge  2  ; GREPFIELD(static/info, 'Partial ionic charges', 3, 3) ; 1.0
match ;  Density value 1    ; LINEFIELD(static/density.y=0\,z=0, 2, 2) ; 0.00969772364353607
match ;  Density value 2    ; LINEFIELD(static/density.y=0\,z=0, 3, 2) ; 0.008619614527950625
match ;  Bader value 1      ; LINEFIELD(static/bader.y=0\,z=0, 6, 2) ; 0.00985488071135864
match ;  Bader value 2      ; LINEFIELD(static/bader.y=0\,z=0, 10, 2) ; 0.01174549883030905
