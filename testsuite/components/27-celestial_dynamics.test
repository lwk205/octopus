# -*- coding: utf-8 mode: shell-script -*-

Test       : Oct-test celestial
Program    : octopus
TestGroups : short-run, components
Enabled    : Yes

Processors: 1
Input: 27-celestial_dynamics.01-three_body.inp

# Check initial positions
Precision: 1e-12
match ; Sun pos x   (t=0) ; LINEFIELD(Sun/td.general/coordinates, 6, 3) ; 0.0
match ; Sun pos y   (t=0) ; LINEFIELD(Sun/td.general/coordinates, 6, 4) ; 0.0
match ; Earth pos x (t=0) ; LINEFIELD(Earth/td.general/coordinates, 6, 3) ; 149598023000.0
match ; Earth pos y (t=0) ; LINEFIELD(Earth/td.general/coordinates, 6, 4) ; 0.0
match ; Moon pos x  (t=0) ; LINEFIELD(Moon/td.general/coordinates, 6, 3) ; 149982422000.0
match ; Moon pos y  (t=0) ; LINEFIELD(Moon/td.general/coordinates, 6, 4) ; 0.0

# Check final positions and velocities
Precision: 1e-9
match ;  Sun pos x   (t=365 days)  ; LINEFIELD(Sun/td.general/coordinates, -1, 3) ; 2.358752200555e+01
match ;  Sun pos y   (t=365 days)  ; LINEFIELD(Sun/td.general/coordinates, -1, 4) ; 2.861013698805e+06
Precision: 1e-12
match ;  Earth pos x (t=365 days)  ; LINEFIELD(Earth/td.general/coordinates, -1, 3) ; 1.496000246958e+11
Precision: 0.002
match ;  Earth pos y (t=365 days)  ; LINEFIELD(Earth/td.general/coordinates, -1, 4) ; -1.454262615902e+09
Precision: 1e-12
match ;  Moon pos x  (t=365 days)  ; LINEFIELD(Moon/td.general/coordinates, -1, 3) ; 1.491807203752e+11
Precision: 0.03
match ;  Moon pos y  (t=365 days)  ; LINEFIELD(Moon/td.general/coordinates, -1, 4) ; -1.767251992055e+09

Precision: 1e-12
match ;  Sun vel x   (t=365 days)  ; LINEFIELD(Sun/td.general/coordinates, -1, 5) ; -9.230104644669e-04
match ;  Sun vel y   (t=365 days)  ; LINEFIELD(Sun/td.general/coordinates, -1, 6) ; 4.317656048601e-06
Precision: 1e-9
match ;  Earth vel x (t=365 days)  ; LINEFIELD(Earth/td.general/coordinates, -1, 5) ; 2.981740652162e+02
Precision: 1e-12
match ;  Earth vel y (t=365 days)  ; LINEFIELD(Earth/td.general/coordinates, -1, 6) ; 2.980081857346e+04
Precision: 1e-7
match ;  Moon vel x  (t=365 days)  ; LINEFIELD(Moon/td.general/coordinates, -1, 5) ; 7.443015148702e+02
match ;  Moon vel y  (t=365 days)  ; LINEFIELD(Moon/td.general/coordinates, -1, 6) ; 2.909156553322e+04
