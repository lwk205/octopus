# -*- coding: utf-8 mode: shell-script -*-

Test       : Oct-test celestial
Program    : octopus
TestGroups : short-run, components
Enabled    : Yes

Processors: 1
Input: 27-celestial_dynamics.01-three_body.inp

# Check initial positions
Precision: 1e-12
match ; Sun pos x   (t=0) ; LINEFIELD(Sun/td.general/coordinates, 6, 3) ; 0.0
match ; Sun pos y   (t=0) ; LINEFIELD(Sun/td.general/coordinates, 6, 4) ; 0.0
match ; Earth pos x (t=0) ; LINEFIELD(Earth/td.general/coordinates, 6, 3) ; 149598023000.0
match ; Earth pos y (t=0) ; LINEFIELD(Earth/td.general/coordinates, 6, 4) ; 0.0
match ; Moon pos x  (t=0) ; LINEFIELD(Moon/td.general/coordinates, 6, 3) ; 149982422000.0
match ; Moon pos y  (t=0) ; LINEFIELD(Moon/td.general/coordinates, 6, 4) ; 0.0

# Check final positions and velocities
Precision: 1e-8
match ;  Sun pos x   (t=365 days)  ; LINEFIELD(Sun/td.general/coordinates, -1, 3) ; 23.587522006009998
match ;  Sun pos y   (t=365 days)  ; LINEFIELD(Sun/td.general/coordinates, -1, 4) ; 2861013.698805
Precision: 1e-12
match ;  Earth pos x (t=365 days)  ; LINEFIELD(Earth/td.general/coordinates, -1, 3) ; 149600024695.8
Precision: 0.003
match ;  Earth pos y (t=365 days)  ; LINEFIELD(Earth/td.general/coordinates, -1, 4) ; -1454262615.9015
Precision: 1e-12
match ;  Moon pos x  (t=365 days)  ; LINEFIELD(Moon/td.general/coordinates, -1, 3) ; 149180720375.2
Precision: 0.03
match ;  Moon pos y  (t=365 days)  ; LINEFIELD(Moon/td.general/coordinates, -1, 4) ; -1767251992.034

Precision: 1e-12
match ;  Sun vel x   (t=365 days)  ; LINEFIELD(Sun/td.general/coordinates, -1, 5) ; -0.00092301046446605
match ;  Sun vel y   (t=365 days)  ; LINEFIELD(Sun/td.general/coordinates, -1, 6) ; 4.317656048319e-06
Precision: 1e-9
match ;  Earth vel x (t=365 days)  ; LINEFIELD(Earth/td.general/coordinates, -1, 5) ; 298.17406521655
match ;  Earth vel y (t=365 days)  ; LINEFIELD(Earth/td.general/coordinates, -1, 6) ; 29800.81857346
Precision: 1e-7
match ;  Moon vel x  (t=365 days)  ; LINEFIELD(Moon/td.general/coordinates, -1, 5) ; 744.3015148345
match ;  Moon vel y  (t=365 days)  ; LINEFIELD(Moon/td.general/coordinates, -1, 6) ; 29091.565533189998
