CalculationMode = test

TestMode = celestial_dynamics

Dimensions = 2

%Systems
 "Sun"   | charged_particle
 "Earth" | classical_particle
 "Moon"  | classical_particle
%

#Initial conditions are taken from https://ssd.jpl.nasa.gov/horizons.cgi#top.

Earth.ClassicleParticleMass = 5.97237e24
%Earth.ClassicleParticleInitialPosition
 149598023000.0 | 0.0
%
%Earth.ClassicleParticleInitialVelocity
 0.0 | 29780.0
%

Moon.TDSystemPropagator = beeman_scf
Moon.ClassicleParticleMass = 7.342e22
%Moon.ClassicleParticleInitialPosition
 149982422000.0 | 0.0
%
%Moon.ClassicleParticleInitialVelocity
 0.0 | 30902.0
%

Sun.ClassicleParticleMass = 1.98855e30
%Sun.ClassicleParticleInitialPosition
 0.0 | 0.0
%
%Sun.ClassicleParticleInitialVelocity
 0.0 | 0.0
%

sampling = 20 # Time-steps per day
days = 365
TDTimeStep = 24*3600/sampling
TDMaxSteps = days*sampling
