# -*- coding: utf-8 mode: shell-script -*-

Test       : Oct-test simulation clock
Program    : octopus
TestGroups : short-run, components
Enabled    : Yes

Processors: 2
Input: 28-simulation_clock.01.inp
match ; simulation_clock_get_tick            ; GREPFIELD(out, 'simulation_clock_get_tick', 2)            ; 0000000001
match ; simulation_clock_get_sim_time        ; GREPFIELD(out, 'simulation_clock_get_sim_time', 2)        ; 1.0000000000
match ; simulation_clock_is_earlier          ; GREPFIELD(out, 'simulation_clock_is_earlier', 2)          ; 0
match ; simulation_clock_is_equal_or_earlier ; GREPFIELD(out, 'simulation_clock_is_equal_or_earlier', 2) ; 0
match ; simulation_clock_is_later            ; GREPFIELD(out, 'simulation_clock_is_later', 2)            ; 1
match ; simulation_clock_is_equal_or_later   ; GREPFIELD(out, 'simulation_clock_is_equal_or_later', 2)   ; 1
match ; simulation_clock_is_equal            ; GREPFIELD(out, 'simulation_clock_is_equal', 2)            ; 0
match ; simulation_clock_is_later_with_step  ; GREPFIELD(out, 'simulation_clock_is_later_with_step', 2)  ; 1
